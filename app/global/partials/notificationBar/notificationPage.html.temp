<!-- Main app functions/content goes inside the <main> section -->
<main class="page-height-container">
    <div class="nav-bar" id="navBarTop">
        <div id="slideMenuToggle" class="col-xs-3">
            <span class="glyphicon glyphicon-menu-hamburger"></span>
        </div>
        <div class="page-heading col-xs-6"><h5>Notifications</h5></div>
    </div>
    <div class="content">
        <div class="body-panel panel-list" id="pendingFriendRequests">
            <h5 class="section-heading">Friend Requests  <span class="badge badge-red">2</span></h5>
            <div id="notificationsFriendRequestList" class="friend-list">
                <div class="well well-grey">
                    <div class="panel-left">
                        <img src="https://s3-ap-southeast-2.amazonaws.com/images.pioneerroad.com.au/profile-photos/1/1441153449598_100x100.jpg" class="profile-photo"/>
                    </div>
                    <div class="panel-right">
                        <h3>Michelle Smith</h3>
                        <div class="inline-form friend-action">
                            <div class="input-group">
                                <button class="btn btn-success accept">Accept</button>
                                <button class="btn btn-danger decline">Decline</button>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="well well-grey">
                    <div class="panel-left">
                        <img src="https://s3-ap-southeast-2.amazonaws.com/images.pioneerroad.com.au/profile-photos/1/1441153449598_100x100.jpg" class="profile-photo" />
                    </div>
                    <div class="panel-right">
                        <h3>Ian and June Smith</h3>
                        <div class="inline-form friend-action">
                            <div class="input-group">
                                <button class="btn btn-success">Accept</button>
                                <button class="btn btn-danger">Decline</button>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
</main>
<!-- End of main section -->

<div class="sitewide-navigation slide-menu page-height-container" id="drawerMenu">
    <ul class="sitewide-nav">
        <li data-target="messages" class="menu-item menu-item-messages" aria-label="Messages"><span class="badge">15</span></li>
        <li data-target="notifications" class="menu-item menu-item-notifications" aria-label="Notifications"><span class="badge">5</span></li>
        <li class="divider" aria-hidden="true"></li>
        <li data-target="home" class="menu-item menu-item-home active" aria-label="Home"></li>
        <li data-target="social" class="menu-item menu-item-social" aria-label="Social Connections"></li>
        <li data-target="places" class="menu-item menu-item-places" aria-label="Places"></li>
        <li data-target="user" class="menu-item menu-item-user" aria-label="My Profile"></li>
    </ul>
</div>

